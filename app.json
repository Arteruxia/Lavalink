{
    "name": "Lavalink",
    "description": "lavalink can host on every where",
    "keywords":[
        "lavalink",
        "wavelink",
        "discord.py",
        "discord.js"
    ],
    "repository": "https://github.com/Arteruxia/Lavalink",
    "env": {
        "PASS": {
            "description": "The password change this to whatever you want it to be.",
            "value": "ArteruxiaHost"
        },
        "JAVA_TOOL_OPTIONS": {
            "description": "This sets the ram to max *don't edit* this variable",
            "value": "-XX:+UseContainerSupport -Xmx500m -Xss256k -XX:CICompilerCount=2 -Dfile.encoding=UTF-8"
        }
    },
    "buildpacks": [
        {
            "url": "heroku/nodejs"
        },
        {
            "url": "heroku/java"
        }
    ]
}
